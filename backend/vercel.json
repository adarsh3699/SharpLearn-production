{
	"version": 2,
	"name": "sharplearn-backend",
	"builds": [
		{
			"src": "./index.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "/"
		}
	],
	"env": {
		"NODE_ENV": "production"
	},
	"functions": {
		"./index.js": {
			"maxDuration": 10
		}
	},
	"regions": ["iad1"]
}
